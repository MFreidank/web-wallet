{
  "workDir": ".",
  "dependencies": [
	{
	  "name": "privatesky",
	  "src": "http://github.com/privatesky/privatesky.git",
	  "actions": [
		{
		  "type": "smartClone",
		  "target": "."
		},
		{
		  "type": "execute",
		  "cmd": "cd ./privatesky && npm install && npm run build"
		}
	  ]
	},
	{
	  "name": "menu-wallet",
	  "src": "http://github.com/privatesky/menu-wallet.git",
	  "actions": [
		{
		  "type": "smartClone",
		  "target": ".",
		  "collectLog": false
		},
		{
		  "type": "execute",
		  "cmd": "cd menu-wallet && npm install"
		}
	  ]
	},
	{
	  "name": "profile-app",
	  "src": "http://github.com/privatesky/profile-app.git",
	  "actions": [
		{
		  "type": "smartClone",
		  "target": ".",
		  "collectLog": false
		},
		{
		  "type": "execute",
		  "cmd": "cd profile-app && npm install"
		}
	  ]
	}
  ]
}